# 1) Image aplikasi dari GitHub Container Registry
KASIR_IMAGE=ghcr.io/indralavansa/kasir-app:v1.0.0

# 2) License server (aktivasi online)
LICENSE_SERVER_URL=http://192.168.1.25:8088
LICENSE_SERVER_PUBLIC_KEY_B64=

# 3) License key (dari penjual)
KASIR_LICENSE_KEY=

# 4) Device fingerprint (isi dari hasil device_fingerprint.bat / device_fingerprint.py)
DEVICE_FINGERPRINT=

# Telegram (hanya untuk Pro/Unlimited)
TELEGRAM_BOT_TOKEN=
TELEGRAM_ADMIN_CHAT_IDS=
TELEGRAM_NOTIFY_NEW_TRANSACTION=false
TELEGRAM_NOTIFY_LOW_STOCK_THRESHOLD=10
